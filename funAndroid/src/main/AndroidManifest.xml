<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cainiao.funandroid">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".FunApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.RxJavaOperatorActivity"></activity>
        <activity android:name=".activity.RxjavaRetrofitActivity" />
        <activity android:name=".activity.MoveIndicatorActivity" />
        <activity android:name=".activity.OKHttpActivity" />
        <activity android:name=".activity.RxJavaActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>